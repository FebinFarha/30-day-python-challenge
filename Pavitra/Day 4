
i = 1
count = 0
while count < 10:
    if i % 2 == 0:
        print(i, end=" ")
        count += 1
    i += 1


i = 1
count = 0
while count < 10:
    if i % 2 != 0:
        print(i, end=" ")
        count += 1
    i += 1


i = 1
while i <= 10:
    print(i, end=" ")
    i += 1

i = 0
while i < 10:
    print(i, end=" ")
    i += 1


i = 1
while i >= 1:
    print(i, end=" ")
    i -= 1


i = 10
while i <= 300:
    print(i, end=" ")
    i += 10